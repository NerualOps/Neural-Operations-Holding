{
  "name": "neuralops-dashboard",
  "version": "1.0.0",
  "description": "NeuralOps secure login and dashboard system",
  "main": "runtime/server.js",
  "scripts": {
    "start": "bash start.sh",
    "start-with-learning": "cd services/python-services && python3 epsilon_learning_service.py & node ../../runtime/server.js",
    "build": "node build.js --obfuscate=true --anti-debug=true",
    "build-dev": "echo 'Build disabled in dev mode to prevent obfuscation'",
    "prebuild": "",
    "dev": "nodemon --max-old-space-size=8192 runtime/server.js",
    "clear-ports": "powershell -ExecutionPolicy Bypass -File clear-ports.ps1",
    "obfuscate": "node build.js --obfuscate=true --anti-debug=true",
    "security-audit": "npm audit --production",
    "lint": "echo 'No linting configured'",
    "security-scan": "node scripts/security-scan.js",
    "security-scan-full": "npm audit --production && node scripts/security-scan.js",
    "update-dependencies": "npm update",
    "render-postbuild": "node build.js --obfuscate=true",
    "copy-supabase": "node -e \"require('fs').promises.copyFile('node_modules/@supabase/supabase-js/dist/umd/supabase.js', 'services/libs/supabase.js').then(() => console.log('Supabase copied')).catch(e => console.error('Error:', e))\"",
    "render-start": "NODE_OPTIONS=\"--max-old-space-size=8192\" node runtime/server.js",
    "python-install": "cd services/python-services && python -m pip install -r requirements.txt",
    "python-dev": "cd services/python-services && python -m uvicorn nlp_processor:app --reload --port 8001",
    "python-start": "cd services/python-services && python nlp_processor.py & python learning_analytics.py & python content_generator.py & python epsilon_learning_service.py"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "@xenova/transformers": "^2.6.2",
    "adm-zip": "^0.5.10",
    "archiver": "^6.0.1",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dompurify": "^3.0.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.9.0",
    "form-data": "^4.0.4",
    "helmet": "^6.2.0",
    "javascript-obfuscator": "^4.0.2",
    "jsdom": "^22.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "terser": "^5.17.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "snyk": "^1.1250.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neuralops/epsilon-ai-system"
  },
  "author": "Neural Operation's & Holding's LLC",
  "license": "MIT"
}
